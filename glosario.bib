% Encoding: UTF-8
@entry{aduana,
  name        = {aduana sanitaria},
  description = {zona destinada a que las personas que entrarán a áreas operativas puedan lavarse y desinfectarse las manos, así como ponerse cubrebocas y cofia con el propósito de evitar riesgos de inocuidad al entrar a dicha área.}
}

@entry{alergeno,
  plural      = {alérgenos},
  name        = {alérgeno},
  description = {un componente conocido en algún alimento que causa reacciones fisiológicas debido a una respuesta inmunológica (\textit{e.g.}\ nueces y otros identificados en la legislación relevante para el país.)
                 \begin{itemize}
                 \item[Nota 1:] La NOM-051-SCFI/SSA1-2010 \textit{(Especificaciones generales de etiquetado para alimentos y bebidas no alcohólicas preenvasados --- Información comercial y sanitaria)} es la que detalla qué componentes se consideran \emph{alérgenos} según la legistlación mexicana.
                 \end{itemize}
                 }
}

@entry{alimento,
  plural      = {alimentos},
  name        = {alimento},
  description = {sustancia (ingrediente), ya sea procesada, semi-procesada o cruda, que se destina para consumo, e incluye bebidas, goma de mascar y cualquier sustancia que se haya utilizado en la fabricación, preparación o tratamiento de “alimentos”, pero no incluye cosméticos ni tabaco o sustancias (ingredientes) usados solamente como fármacos}
}

@abbreviation{alimento-RTE,
  description = {alimentos preparados por el productor para su consumo directo sin necesidad de cocinarlos ni someterlos a ningún tratamiento.},
  short       = {RTE},
  first       = {alimento listo para el consumo, del inglés \textit{ready-to-eat} (RTE)},
  long        = {\textit{ready-to-eat}}
}

@entry{alta-direccion,
  name        = {alta dirección},
  description = {persona o grupo de personas que dirige y controla una organización al más alto nivel
                 \begin{itemize}
                 \item[Nota 1] La alta dirección tiene el poder para delegar autoridad y proporcionar recursos dentro de la organización.
                 \item[Nota 2] Si el alcance del sistema de gestión comprende solo una parte de una organización entonces la alta dirección se refiere a quienes dirigen y controlan esa parte de la organización.
                 \end{itemize}
                 }
}

@entry{area-operativa,
  name        = {área operativa},
  description = {zona destinada a que ocurran las actividades operativas; en nuestro caso, el almacenamiento de producto y maniobras asociadas.}
}

@acronym{BPD,
  short = {BPD},
  long  = {Buenas Prácticas de Distribución}
}

@acronym{BPH,
  short = {BPH},
  long  = {Buenas Prácticas de Higiene}
}

@entry{cadena-alimentaria,
  name        = {cadena alimentaria},
  description = {secuencia de etapas en la producción, procesamiento, distribución, almacenamiento, y manipulación de un \glsname{alimento} y sus ingredientes, desde la producción primaria hasta el consumo.
                 \begin{itemize}
                 \item[Nota 1] La cadena alimentaria incluye la producción de materiales destinados a entrar en contacto con alimentos o materias primas.
                 \item[Nota 2] La cadena alimentaria también incluye proveedores de servicio.
                 \end{itemize}}
}

@entry{cantidades-pequeñas,
  name        = {cantidades pequeñas},
  description = {en el proceso de entrega, se refiere a una cantidad de \glsname{producto} total que no alcance a llenarse una tarima completa de \glsname{producto} pre-empacado}
}

@abbreviation{CFE,
  short = {CFE},
  long  = {Comisión Federal de Electricidad}
}

@entry{cliente,
  plural      = {clientes},
  name        = {cliente},
  description = {persona ajena a la operación cotidiana en RDF, que tiene un acuerdo con RDF para almacenar productos en sus instalaciones.}
}

@entry{congelacion,
  name        = {congelación},
  description = {método físico que se efectúa por medio de equipo especial para lograr una reducción de la temperatura de los productos que garantice la solidificación del agua contenida en estos. La congelación debe de ser por debajo de \cels{-15}, hasta \cels{-30}, según el tipo de producto}
}

@entry{contaminacion-cruzada,
  name        = {contaminación cruzada},
  description = {proceso por el cual se transfieren involuntariamente microorganismos de una sustancia u objeto a otro, \emph{con efectos nocivos}}
}

@entry{criterio-de-acción,
  plural      = {criterios de acción},
  name        = {criterio de acción},
  description = {especificación medible u observable para el seguimiento de un \glsname{PPRO}
                 \begin{itemize} 
                 \item[Nota:] Un criterio de acción se establece para determinar si un PPRO permanece bajo control, y distingue entre lo que es aceptable (que el criterio cumpla o logre significa que el PPRO está operando como está previsto) e inaceptable (que el criterio no se cumple ni se logre significa que el PPRO no está operando como está previsto)
                 \end{itemize}}
}

@entry{desastre-natural,
  plural      = {desastres naturales},
  name        = {desastre natural},
  description = {hace referencia a las enormes pérdidas, materiales y vidas humanas, ocasionadas por eventos o fenómenos naturales como los terremotos, inundaciones, huracanes y otros}
}

@entry{documento,
  plural      = {documentos},
  name        = {documento},
  description = {\glsname{informacion} y el medio en el que está contenida
                 \begin{itemize}
                 \item[Nota 1] El medio de soporte puede ser papel, disco magnético, electrónico u óptico, fotografía o muestra patrón o una combinación de éstos;
                 \item[Nota 2] Con frecuencia, un conjunto de documentos, por ejemplo especificaciones y registros, se denominan “documentación”;
                 \item[Nota 3] Algunos requisitos (por ejemplo, el requisito de ser legible) se refieren a todo tipo de documento. Sin embargo puede requisitos diferentes para las especificaciones (por ejemplo, el requisito de estar controlado por revisiones) y los registros (por ejemplo, el requisito de ser recuperable).
                 \end{itemize}
                 }
}

@entry{emergencia,
  plural      = {emergencias},
  name        = {emergencia},
  description = {es una situación fuera de control que se presenta por el impacto de un desastre}
}

@entry{especificacion,
  plural      = {especificaciones},
  name        = {especificación},
  description = {\glsname{documento} que establece requisitos
                 \begin{itemize}
                 \item[Nota 1] Manual de la calidad, plan de la calidad, plano técnico, documento de procedimiento, instrucción de trabajo.
                 \item[Nota 2] Una especificación puede estar relacionada con actividades (por ejemplo, un documento de procedimiento una especificación de proceso y una especificación de ensayo, o con productos (por ejemplo, una especificación de producto, una especificación de desempeño y un plano)).
                 \item[Nota 3] Puede que, al establecer requisitos una especificación esté estableciendo adicionalmente resultados logrados por el diseño y desarrollo y de este modo en algunos casos puede utilizarse como un registro
                 \end{itemize}
                 }
}

@entry{FD,
  name        = {defensa alimentaria},
  description = {Según la definición de la Administración de Alimentos y Fármacos de los Estados Unidos, son los esfuerzos para evitar la contaminación intencional de los alimentos por peligros biológicos, físicos, químicos o radiológicos que no pueden ocurrir, de forma razonable, en el suministro de alimentos},
  first       = {defensa alimentaria, también conocido por su nombre en inglés \textit{"Food Defense"}}
}

@acronym{HACCP,
  short = {HACCP},
  long  = {Analisis de Peligros y Puntos Criticos de Control (APPC), del inglés \textit{Hazard Analysis and Critical Control Points (HACCP)}}
}

@entry{idoneidad-de-los-alimentos,
  name        = {idoneidad de los alimentos},
  description = {Garantía de que los alimentos son aceptables para el consumo humano de acuerdo con su uso previsto.}
}

@entry{informacion,
  name        = {información},
  description = {datos que poseen significado}
}

@entry{informacion-documentada,
  name        = {información documentada},
  description = {\glsname{informacion} que una \glsname{organizacion} tiene que controlar y mantener, y el medio que la contiene
                 \begin{itemize}
                 \item[Nota 1] La información documentada puede estar en cualquier formato y medio, y puede provenir de cualquier fuente;\item[Nota 2] La información documentada puede hacer referencia a:
                 \begin{itemize}
                 \item el sistema de gestión, incluidos los procesos relacionados;
                 \item la información generada para que la organización opere (documentación);
                 \item la evidencia de los resultados alcanzados (\glsname{registro}).
                 \end{itemize}
                 \end{itemize}}
}

@entry{inocuidad-de-los-alimentos,
  name        = {inocuidad de los alimentos},
  description = {Garantía de que los alimentos no causarán efectos adversos en la salud del consumidor cuando se preparen o se consuman de acuerdo a su uso previsto.}
}

@entry{inventario-de-suministros-y-materiales,
  name        = {inventario de suministros y materiales},
  description = {la contemplación de las cantidades de inventario asegura que la compañía tiene materiales a la mano para hacer productos o brindar un servicio adecuado y que los fondos no son desperdiciados en materiales innecesarios. Una cuenta precisa de inventario también permite que las compañías controlen y ordenen suficientes materiales para la demanda que amerita la operación.}
}

@entry{limpieza,
  name        = {limpieza},
  description = {Proceso de remover los residuos orgánicos y suciedad que pueden ser una fuente de contaminación. Los métodos de limpieza adecuados y los materiales dependerán de la naturaleza del alimento}
}

@acronym{LPA,
  short = {LPA},
  long  = {Lista de Proveedores Autorizados}
}

@entry{manual-de-calidad,
  name        = {manual de calidad},
  description = {\glsname{especificacion} para el sistema de gestión de la calidad de una organización
                 \begin{itemize}
                 \item[Nota] Los manuales de la calidad pueden variar en cuanto a detalle y formato para adecuarse al tamaño y complejidad de cada \glsname{organizacion} en particular.
                 \end{itemize}
                 }
}

@entry{medida-de-control,
  name        = {medida de control},
  description = {acción o actividad que es esencial para prevenir un peligro relacionado con la inocuidad de los \glsname{alimento} significativo o reducirlo a un \glsname{nivel-aceptable}}
}

@entry{monitoreo,
  plural      = {monitoreos},
  name        = {monitoreo},
  description = {El proceso de la aplicación de una secuencia planeada de observaciones o mediciones para evaluar si una medida o combinación de medidas de control están o no funcionando de la manera prevista y cumpliendo con los límites especificados}
}

@abbreviation{MSDS,
  description = {Documento que imprime información sobre los compuestos químicos que contiene un determinado, así como también instrucciones de uso, información sobre los peligros y riesgos asociados, condiciones de almacenaje, indicaciones y medidas a tomar para casos de emergencia, etc.},
  short       = {MSDS},
  first       = {Hoja de Seguridad de Material, del inglés Material Safety Data Sheet (MSDS)},
  long        = {Hoja de Seguridad de Material}
}

@entry{nivel-aceptable,
  name        = {nivel aceptable},
  description = {nivel de un peligro relacionado con la \glsname{inocuidad-de-los-alimentos} que no se debe exceder en el \glsname{producto-terminado} proporcionado por la \glsname{organizacion}}
}

@entry{organizacion,
  name        = {organización},
  description = {persona o grupo de personas que tienen sus propias funciones con responsabilidades, autoridades y relaciones para lograr sus objetivos.
                 \begin{itemize}
                 \item[Nota] El concepto de organización incluye, pero no se limita a, un operador individual, compañía, corporación, firma, empresa, autoridad, sociedad, institución benéfica o con otros fines, o parte o combinación de los mismos, ya sea incorporada o no, pública o privada.
                 \end{itemize}
                 }
}

@entry{peligro-relacionado-con-la-inocuidad-de-los-alimentos,
  name        = {peligro relacionado con la inocuidad de los alimentos},
  description = {agente biológico, químico o físico en el \glsname{alimento} con potencial de causar un efecto adverso en la salud
                 \begin{itemize}
                 \item[Nota 1] El término “peligro” no se debe confundir con el término “riesgo” el cual, en el contexto de la inocuidad de los alimentos, significa una función de la probabilidad de un efecto adverso en la salud (por ejemplo, enfermar) y la gravedad de ese efecto (por ejemplo, muerte, hospitalización) cuando se expone a un peligro especificado;
                 \item[Nota 2] Peligros para la inocuidad de los alimentos incluye alérgenos y sustancias radiológicas.
                 \end{itemize}
                 }
}

@abbreviation{PEPS,
  short = {PEPS},
  long  = {Primeras Entradas, Primeras Salidas}
}

@entry{personal-interno,
  name        = {personal interno},
  description = {persona contratada por RDF}
}

@entry{plan-de-la-calidad,
  name        = {plan de la calidad},
  description = {especificación de los procedimientos y recursos asociados a aplicar, cuándo deben aplicarse y quién debe aplicarlos a un objeto específico}
}

@entry{politica,
  name        = {politica},
  description = {(organización) intenciones y dirección de una organización, como las expresa formalmente su alta dirección.}
}

@entry{politica-de-calidad,
  name        = {política de calidad},
  description = {política relativa a la calidad
                 \begin{itemize}
                 \item[Nota 1] Generalmente la política de la calidad es coherente con la política global de la organización, puede alinearse con la visión y la misión de la organización y proporciona un marco de referencia para el establecimiento de los objetivos de la calidad.
                 \end{itemize}
                 }
}

@acronym{PPR,
  longplural  = {programas de prerrequisitos},
  description = {condiciones y actividades básicas que son necesarias dentro de la \glsname{organizacion} y a lo largo de la cadena alimentaria para mantener la inocuidad de los alimentos
                 \begin{itemize}
                 \item[Nota 1] Los PPR necesarios dependen del segmento de la cadena alimentaria en el que opera la organización y del tipo de organización;
                 \item[Nota 2] Son ejemplos de términos equivalentes: Buenas Prácticas Agrícolas (BPA), Buenas Prácticas Veterinarias (BPV), Buenas Prácticas de Fabricación/Manufactura (BPF, BPM), Buenas Prácticas de Higiene (BPH), Buenas Prácticas de Producción (BPP), Buenas Prácticas de Distribución (BPD), y Buenas Prácticas de Comercialización (BPC).
                 \end{itemize}
                 },
  short       = {PPR},
  long        = {programa de prerrequisito}
}

@acronym{PPRO,
  description = {\glsname{medida-de-control} o combinación de medidas de control aplicadas para prevenir o reducir un peligro significativo relacionado con la inocuidad de los \glsname{alimento} a un nivel-aceptable, y donde el \glsname{criterio-de-acción} y medición u observación permite el control efectivo del \glsname{proceso} y/o \glsname{producto}},
  short       = {PPRO},
  long        = {Programa de Prerrequisitos Operativos}
}

@entry{proceso,
  name        = {proceso},
  description = {conjunto de actividades interrelacionadas o que interactúan que transforman las entradas en salidas}
}

@entry{producto,
  name        = {producto},
  description = {salida que es el resultado de un \glsname{proceso}
                 \begin{itemize}
                 \item[Nota 1:]{Un producto puede ser un servicio.}
                 \end{itemize}}
}

@entry{producto-terminado,
  name        = {producto terminado},
  description = {\glsname{producto} que no se someterá a procesamiento o transformación posterior por parte de la organización
                 \begin{itemize}
                 \item[Nota] Un producto que es sometido a un procesamiento o transformación posterior por otra organización, es un producto terminado, en el contexto de la primera organización, y una materia prima o un ingrediente, en el contexto de la segunda organización
                 \end{itemize}
                 }
}

@entry{proteina-cruda,
  name        = {proteína cruda},
  description = {resultado de un análisis químico que se realiza en los alimentos, basados en el nitrógeno, para obtener la cantidad de proteínas que este contiene.}
}

@entry{proveedor,
  plural      = {proveedores},
  name        = {proveedor},
  description = {organización que proporciona un producto o un servicio
                 \begin{itemize}
                 \item[Nota 1] Un proveedor puede ser interno o externo a la organización.
                 \item[Nota 2] En una situación contractual, un proveedor puede denominarse a veces "contratista".
                 \end{itemize}}
}

@entry{proveedor-externo,
  plural      = {proveedores externos},
  name        = {proveedor externo},
  description = {proveedor que no es parte de la organización.
                 \begin{itemize}
                 \item[Ejemplo:] Productor, distribuidor, minorista o vendedor de un producto, o un servicio.
                 \end{itemize}
                 }
}

@abbreviation{PSA,
  short = {PSA},
  long  = {Programa de Seguridad Alimentaria}
}

@acronym{RDF,
  short = {RDF},
  long  = {Red de Fríos S.A. de C.V.}
}

@entry{recuperación-de-producto,
  name        = {recuperación de producto},
  description = {El retiro de un producto de la cadena de suministro por parte del proveedor que se ha declarado como no inocuo, se ha vendido al consumidor final y está disponible para la venta}
}

@entry{refrigeracion,
  name        = {refrigeración},
  description = {método físico de conservación con el cual se mantiene una temperatura interna de un producto a máximo \cels{4}}
}

@entry{registro,
  plural      = {registros},
  name        = {registro},
  description = {\glsname{documento} que presenta resultados obtenidos o proporciona evidencia de actividades realizadas
                 \begin{itemize}
                 \item[Nota 1] Los registros pueden utilizarse, por ejemplo, para formalizar la trazabilidad y para proporcionar evidencia de verificaciones, acciones preventivas y acciones correctivas.
                 \item[Nota 2] En general los registros no necesitan estar sujetos al control del estado de revisión.
                 \end{itemize}}
}

@entry{requisito,
  plural      = {requisitos},
  name        = {requisito},
  description = {necesidad o expectativa establecida, generalmente implícita u obligatoria
                 \begin{itemize}
                 \item[Nota 1] “Generalmente implícita” significa que es habitual o práctica común para la \glsname{organizacion} y las partes interesadas el que la necesidad o expectativa bajo consideración está implícita.
                 \item[Nota 2] Un requisito especificado es aquel que está establecido, por ejemplo, en \glsname{informacion-documentada}
                 \item[Nota 3] Pueden utilizarse calificativos para identificar un tipo específico de requisito, (por ejemplo, \textit{requisito}) de un \glsname{producto}, requisito de la gestión de la calidad, requisito del cliente, requisito de la calidad.
                 \end{itemize}
                 }
}

@entry{retiro-de-producto,
  name        = {retiro de producto},
  description = {El retiro de un producto de la cadena de suministro por parte del proveedor que se ha declarado como no inocuo y que no se ha puesto a la venta en el mercado para el consumidor final}
}

@entry{revision-documental,
  plural      = {revisiónes documentales},
  name        = {revisión documental},
  description = {proceso de actualización de la información documentada contenida en algun programa al determinarse que se necesita algún requerimiento adicional o hubo algún cambio significativo.}
}

@acronym{SADER,
  short = {SADER},
  long  = {Secretaría de Agricultura y Desarrollo Rural}
}

@entry{sanitizacion,
  name        = {sanitización},
  description = {Proceso realizado después de efectuar la limpieza profunda de equipos, maquinaria e instalaciones la cual nos permite el reducir el número de microorganismos}
}

@entry{seguimiento,
  name        = {seguimiento},
  description = {determinación del estado de un sistema, un \glsname{proceso} o una actividad
                 \begin{itemize}
                 \item[Nota 1] Para determinar el estado, puede ser necesario verificar, supervisar u observar críticamente.
                 \item[Nota 2] En el contexto de la inocuidad de los alimentos, el seguimiento se lleva a cabo con una secuencia planificada de observaciones o mediciones para evaluar si un proceso está funcionando según lo previsto.
                 \end{itemize}}
}

@abbreviation{SENASICA,
  short = {SENASICA},
  long  = {Servicio Nacional de Sanidad, Inocuidad y Calidad Agroalimentaria}
}

@abbreviation{SGA,
  description = {parte de un sistema de gestión relacionada con los inventarios del almacén},
  short       = {SGA},
  long        = {Sistema de Gestión del Almacén}
}

@abbreviation{SGC,
  description = {parte de un sistema de gestión relacionada con la calidad},
  short       = {SGC},
  long        = {Sistema de Gestión de Calidad}
}

@entry{sistema-de-gestion,
  name        = {sistema de gestión},
  description = {conjunto de elementos de una \glsname{organizacion} interrelacionados o que interactúan para establecer políticas, objetivos y procesos para lograr estos objetivos.}
}

@entry{tarima-compuesta,
  plural      = {tarimas compuestas},
  name        = {tarima compuesta},
  description = {una unidad que contiene alimento de diferente tipo, correspondiente al mismo cliente de RDF.
                 \begin{itemize}
                 \item[Nota:] \glsname{RDF} no tiene el privilegio de armar tarimas compuestas a menos que el cliente lo solicite.
                 \end{itemize}}
}

@entry{uniforme-completo,
  name        = {uniforme completo},
  description = {se considera como \textit{uniforme completo} al uso de zapatos cerrados, cubrebocas, cofia y a un conjunto de chamarra y pantalones para protección contra el freio o en su defecto un overlo. Los guantes son opcionales pero recomendados}
}

@entry{validacion-documental,
  name        = {validación documental},
  description = {proceso en el que se verifica que la información documentada contemple los requerimientos actuales del SGC. De ser así, no se requerirá de una revisión}
}

@entry{visitante,
  plural      = {visitantes},
  name        = {visitante},
  description = {persona que accederá a las instalaciones para brindar un servicio, o bien, personal contratado por un \emph{cliente} que laborará constantemente dentro de las instalaciones de RDF y tiene que someterse al reglamento interno}
}